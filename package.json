{
  "name": "elements-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod --output-hashing=none",
    "package-es5": "cat ./dist/elements-demo/runtime-es5.js ./dist/elements-demo/polyfills-es5.js ./dist/elements-demo/scripts.js ./dist/elements-demo/main-es5.js > src/custom-button-element.js",
    "package-es5-with-tslint-set-target-es5": "cat ./dist/elements-demo/runtime.js ./dist/elements-demo/polyfills.js ./dist/elements-demo/scripts.js ./dist/elements-demo/main.js > src/custom-button-element.js",
    "package-es2015": "cat ./dist/elements-demo/runtime-es2015.js ./dist/elements-demo/polyfills-es2015.js ./dist/elements-demo/scripts.js ./dist/elements-demo/main-es2015.js > src/custom-button-element.js",

    "button-es5": "npm run build && npm run package-es5",

    "button-es2015": "npm run build && npm run package-es2015"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "document-register-element": "1.8.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
